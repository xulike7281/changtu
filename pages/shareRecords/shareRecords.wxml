<!--pages/shareRecords/shareRecords.wxml-->
<view class="container">
  <view class='share-wrap bg-fff'>
    <view class='share-li row cen-center ' wx:for="{{shareList}}" wx:key="{{index}}">
      <view>
        <image src='../../static/img/share_author_icon.png'></image>
      </view>
      <view class='equal'>
        <view class='row cen-space'>
          <text class='ft14 color-323232'>成功邀请<text class='color-ff0000'>{{item.sjhm}}</text></text>
          <text class='ft12 color-999'>{{item.create_ts}}</text>
        </view>
        <view class='row cen-space'>
        <text class='ft12 color-999'>{{item.msg}}</text>
        <text class='ft12 color-323232'>{{item.msg2}}</text>
        </view>
      </view>
    </view>
  </view>
</view>